package emptyLib._3D.Tutorials.away3d   {	import emptyLib._3D.Template;	import away3d.primitives.Sphere;		public class LocalAxisMovement extends Template {		public function LocalAxisMovement() {			super();		}				protected override function initScene():void{			super.initScene();			var sphere:Sphere = new Sphere({x:400,y:-300,y:500});			this.scene.addChild(sphere);			sphere.rotationY = -90;			sphere.moveRight(850);			sphere.pitch(90);		}	}	}