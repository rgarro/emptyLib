package emptyLib._3D.Tutorials.away3d  {	import emptyLib._3D.Template;	import away3d.containers.ObjectContainer3D;	import away3d.primitives.Sphere;		public class GroupingExample extends Template {		public function GroupingExample() {			super();		}		protected override function initScene():void{			super.initScene();			var container:ObjectContainer3D = new ObjectContainer3D({x:0,y:0,z:500});			this.scene.addChild(container);			var sphere:Sphere = new Sphere({x:0,y:0,z:0});			container.addChild(sphere);		}	}	}